{
  "languageOptions": {
    "globals": {
      "cy": "readonly",
      "context": "readonly",
      "beforeEach": "readonly",
      "afterEach": "readonly",
      "window": "readonly",
      "document": "readonly",
      "browser": true,
      "node": true,
      "commonjs": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["**/*.cy.js", "**/*.cy.jsx"],
      "env": {
        "browser": true,
        "node": true,
        "cypress/globals": true
      },
      "plugins": ["cypress"],
      "extends": ["plugin:cypress/recommended"],
      "rules": {
      }
    }
  ]
}
